#include "avl.h"
#include <stdlib.h>
#include <stdio.h>


int main(void){

	node *root = NULL;
	long int status,data,check;
	printf("Enter 0 to search and 1 for insert and 2 for delete\n");
		while(1){
			scanf("%ld",&check);
			switch(check){
				case 0:
				scanf("%ld",&data);
				search(&root,data);
				break;

				case 1:
				scanf("%ld",&data);
				status = insert(&root,data);
				if(status !=0)
				printf("not Inserted");
				break;

				case 2:
				scanf("%ld",&data);
				status = delete(&root,data);
				if(status!=0)
				printf("element not exists");
				break;

				case 9:
				Inorder(root);
				printf("\n");
				break;


				default :
				break;
			}
		}
	}
